{
    "name": "diego-brocanelli/calculate-working-days",
    "description": "Responsible for calculating working days between a date range",
    "type": "component",
    "license": "MIT",
    "authors": [
        {
            "name": "Diego Brocanelli",
            "email": "contato@diegobrocanelli.com.br"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "DiegoBrocanelli\\": "src/"
        }
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3@dev",
        "symfony/var-dumper": "^5.0@dev",
        "phpstan/phpstan": "0.12.x-dev"
    },
    "scripts":{
        "server":"php -S localhost:8080 public/index.php",
        "tests":"vendor/bin/phpunit tests/",
        "code-analysis":"vendor/bin/phpstan analyse src tests --level=4"
    }
}
